{
    "routes": [
      {
        "route": "/index.html"
      },
      {
        "route": "/industries/inpres-industries/index.html"
      }
    ],
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": ["/static/media/*.{png,jpg,gif,svg}", "/static/css/*"]
    },
    "responseOverrides": {
      "400": {
        "rewrite": "/404.html"
      },
      "403": {
        "rewrite": "/404.html"
      },
      "404": {
        "rewrite": "/404.html"
      }
    }
  }